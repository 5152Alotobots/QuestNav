{
  "metadata": [
    {
      "src": [
        {
          "files": ["QuestNav.csproj"],
          "src": "../"
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": true,
      "namespaceLayout": "flattened",
      "memberLayout": "samePage",
      "allowCompilationErrors": true,
      "properties": {
        "TargetFramework": "netstandard2.1"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "../../docs/static/api/csharp",
    "template": ["default"],
    "globalMetadata": {
      "_appTitle": "QuestNav API Documentation",
      "_appName": "QuestNav API",
      "_appFooter": "QuestNav - VR Navigation for FRC Robots",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": true
    }
  }
}
